{
  "expo": {
    "name": "Dude whereâ€™s my car?",
    "slug": "find-anything",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.basedhardware.find",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification",
          "bluetooth-central"
        ],
        "NSBluetoothAlwaysUsageDescription": "This app needs Bluetooth to connect to OMI Glass devices"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.basedhardware.find",
      "permissions": [
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": ["peripheral", "central"],
          "bluetoothAlwaysPermission": "Allow this device to connect to OMI Glass devices"
        }
      ]
    ],
    "developmentClient": {
      "silentLaunch": false
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "experiments": {
      "baseUrl": "/llama3-hackaton"
    }
  }
}
