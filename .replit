modules = ["c-clang14", "cpp-clang14", "nodejs-20", "python-3.11", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["redis"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Zeke Core API"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Zeke Dashboard"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Redis Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Zeke Celery Worker"

[[workflows.workflow]]
name = "Zeke Core API"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd zeke-core && python main.py"
waitForPort = 8000

[[workflows.workflow]]
name = "Zeke Dashboard"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd zeke-core/dashboard && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Redis Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "redis-server --port 6379"

[[workflows.workflow]]
name = "Zeke Celery Worker"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd zeke-core && python worker.py"

[workflows.workflow.metadata]
outputType = "console"

